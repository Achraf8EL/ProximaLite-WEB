@page
@model ProximaLite.Web.Pages.Processes.CreateModel

<h1>Create process</h1>

<div class="card">
  <form method="post">
    <div>
      <label asp-for="Process.Name"></label>
      <input id="process-name" asp-for="Process.Name" />
      <span asp-validation-for="Process.Name"></span>
    </div>

    <div>
      <label asp-for="Process.Description"></label>
      <textarea id="process-description" asp-for="Process.Description"></textarea>
    </div>

    <button id="create-process-btn" type="submit">Save</button>
  </form>
</div>

<p><a asp-page="Index">Back</a></p>
